@using MvcCv_.Models.Entity
@model List<TblDeneyimlerim>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Deneyim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach(var x in Model)
            {
        <tr>
            <td>@x.ID</td>
            <td>@x.Baslik</td>
            <td>@x.AltBaslik</td>
            <td>@x.Tarih</td>
            <td><a href="/Deneyim/DeneyimSil/@x.ID" class="btn btn-primary">Sil</a></td>
            <td><a href="/Deneyim/DeneyimGetir/@x.ID" class="btn btn-warning">Düzenle</a></td>
            
        </tr>

            }
    <tr></tr>
</table>
<a href="/Deneyim/DeneyimEkle/" class="btn btn-danger">Yeni Deneyim Ekle</a>